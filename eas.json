{
  "cli": {
    "version": ">= 10.2.4"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "preview": {
      "distribution": "internal"
    },
    "staging": {
      "env": {
        "EXPO_ENV": "staging"
      },
      "image": "default",
      "node": "16.x",
      "yarn": "classic"
    },
    "production": {
      "env": {
        "EXPO_ENV": "production"
      },
      "image": "default",
      "node": "16.x",
      "yarn": "classic"
    }
  },
  "submit": {
    "production": {},
    "staging": {}
  }
}
